<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/css/jdThemes.css" rel="stylesheet" />
    <link href="~/Content/css/animate.css" rel="stylesheet" />
</head>
<body id="page-top">
    <header>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="navbar-content">
                <div class="navbar-header-left">
                    <div class="navbar-brand">
                        <a href="#">
                            <div class="logo-div"></div>
                        </a>
                    </div>

                    @*<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>*@
                    @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                </div>
                <div class="navbar-header-right">
                    <ul id="nav" >
                        <li>
                            <button type="button" id="menu-toggle-btn">
                                <span ></span>
                                <span ></span>
                                <span ></span>
                            </button>
                        </li>
                        <li>
                            <span class="app-title">Data Research Tool</span>
                        </li>
                        <li>
                            <button type="button" class="user-signout-button custom-tooltip" data-toggle="tooltip" title="Logout" data-placement="bottom">
                                <i class="fa fa-sign-out"></i>
                            </button>
                            <button type="button" class="user-noti-button custom-tooltip" data-toggle="tooltip" title="Notification" data-placement="bottom">
                                <i class="fa fa-bell"></i>
                            </button>
                            <div class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#" id="linkUserProfile">
                                    <i class="fa fa-fw fa-user-o"></i>
                                    <span>&nbsp;Data Research User<span class="caret"></span></span>
                                </a>
                                <div class="dropdown-menu">
                                    <a href="https://www.google.com" target="_blank"><i class="fa fa-fw fa-user"></i>User Profile</a>
                                    <a href="#"><i class="fa fa-fw fa-key"></i>Change Password</a>
                                </div>

                            </div>

                            @*<a href="#" class="top-link-logout">Logoff</a>*@
                        </li>
                    </ul>
                </div>


                @*<div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                        @Html.Partial("_LoginPartial")
                    </div>*@
            </div>
        </div>
        @*<div class="breadcrumb">Test</div>*@
    </header>
    <div class="wrapper">
        <nav id="sidebar" class="sidebar-large">
            <ul class="nav navbar-nav">
                <li class="navbar-list">
                    <a href="#" >
                        <i class="fa fa-fw fa-tachometer"></i>
                        <span>Dashboard</span>
                    </a>
                </li>
                <li class="navbar-list">
                    <a href="#" class="has-dropdown active" >
                        <i class="fa fa-fw fa-folder" title="Data Research"></i>
                        <span>Data Research</span>
                    </a>
                    <div class="nav-dropdown">
                        <a href="#" class="active">Projects</a>
                        <a href="#">Data Researchers</a>
                        <a href="#">Dropdown Item 3</a>
                        <a href="#">Dropdown Item 4</a>
                        <a href="#">Dropdown Item 5</a>
                    </div>
                </li>
                <li class="navbar-list">
                    <a href="#" class="has-dropdown">
                        <i class="fa fa-fw fa-users"></i>
                        <span>Administration</span>
                    </a>
                    <div class="nav-dropdown">
                        <a href="#">Dropdown Item 1</a>
                        <a href="#">Dropdown Item 2</a>
                        <a href="#">Dropdown Item 3</a>
                    </div>

                </li>
            </ul>
        </nav>
        <a href="#" class="sidebar-toggle-btn sidebar-toggle-btn-large" >
            <div class="bars">
                <span></span>
                <span></span>
                <span></span>
            </div>
            @*<i class="fa fa-arrow-left"></i>*@
        </a>
        <div class="container-fluid page-content page-content-large">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>
        <!-- Scroll to Top Button-->
        <a class="scroll-to-top" href="#page-top">
            <i class="fa fa-angle-up"></i>
        </a>

    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/jdThemes.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
